@page "/healthcheck_"

<style>

    .wrapper {
        width: 100%;
        height: 100%;
        background-color: blue;
        align-items: center;
        justify-content: center;
        text-align: center;
    }

    .element-container{
        align-items: center;
        justify-content: center;
    }

    .no-symptoms {
        width: 50px;
        height: 50px;
        background-color: red;
        background-color: green;
        padding: 10px;
    }

    .flu-symptoms {
        width: 50px;
        height: 50px;
        background-color: red;
    }

    .covid-symptoms {
        width: 50px;
        height: 50px;
        background-color: red;
    }

    .tested-positive {
        width: 50px;
        height: 50px;
        background-color: red;
    }

    .tested-negative {
        width: 50px;
        height: 50px;
        background-color: red;
    }

</style>

<div class="wrapper">

    <div>
        Please indicate your heatlh status
    </div>

    <div class="element-container">

        <div class="no-symptoms"></div>

        <div class="flu-symptoms"></div>

        <div class="covid-symptoms"></div>

        <div class="tested-positive"></div>

        <div class="tested-negative"></div>

    </div>



</div>

